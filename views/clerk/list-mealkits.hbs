<div class=" row clerk-list px-5 pb-1 pt-3">
<a class="col-sm-2" href="/load-data/meal-kits"><h3>Load Mealkits</h3></a>
<a class= "col-sm-2"href="/clerk/add-mealkit"><h3>Add Mealkit</h3></a>
</div>
<h3 class="px-3 py-3">{{message}}</h3>

<div class="row mx-5 pb-5  clerk-list">
 <table >
    <tr>
     <th ><div class=" py-2 px-3"><h3>Title</h3></div></th>
    <th ><div class=" py-2 px-3"><h3>Includes</h3></div></th>
    <th ><div class=" py-2 px-3"><h3>Description</h3></div></th>
    <th ><div class=" py-2 px-3"><h3>Servings</h3></div></th>
    <th ><div class=" py-2 px-3"><h3>Cooking Time</h3></div></th>
    <th ><div class=" py-2 px-3"><h3>Price</h3></div></th>
    <th ><div class=" py-2 px-3"><h3>Image</h3></div></th>
    <th ><div class=" py-2 px-3"><h3>Edit</h3></div></th>
    <th ><div class=" py-2 px-3"><h3>Delete</h3></div></th>
  </tr>
 
{{#each meal}}

   
  <tr>
    <td><div class=" py-2 px-3"><h3>{{title}}</h3></div></td>
    <td><div class=" py-2 px-3"><h3>{{includes}}</h3></div></td>
    <td><div class=" py-2 px-3"><h3>{{desc}}</h3></div></td>
    <td><div class=" py-2 px-3"><h3>{{servings}}</h3></div></td>
    <td><div class=" py-2 px-3"><h3>{{cookingTime}}</h3></div></td>
    <td><div class=" py-2 px-3"><h3>${{price}}</h3></div></td>
    <th><div class=" py-2 px-3"><img src="/images/Menu/{{imageUrl}}"></th>
    <th><div class=" py-2 px-3"><a href="/clerk/edit-mealkit/:{{_id}}"><h3>Edit Meal</h3></a></div></th>
    <th><div class=" py-2 px-3"><a href="/clerk/remove-mealkit/:{{_id}}"><h3>Delete Meal</h3></a></div></th>
  
   </tr>
  




{{/each}}
  </table>
  </div>


